{
  "version": 2,
  "name": "sqli-ctf-challenge",
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "FLAG_REAL": "Congratulations ! You found the real flag !"
  }
}
