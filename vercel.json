{
  "version": 2,
  "name": "sqli-ctf-challenge",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/login",
      "dest": "/server.js"
    },
    {
      "src": "/submit",
      "dest": "/server.js"
    },
    {
      "src": "/hint/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/ui-log",
      "dest": "/server.js"
    },
    {
      "src": "/inspect.png",
      "dest": "/server.js"
    },
    {
      "src": "/dashboard.html/inspect.png",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "FLAG_REAL": "Congratulations ! You found the real flag !"
  }
}
